<%- include('../partials/header') %>

<h2>All Artists</h2>
<table>
  <thead>
    <tr>
      <th>Artist</th>  
      <th>Genre</th>    
      <th>Year Formed</th>
    </tr>
  </thead>
  <tbody>
    <% artists.forEach(function(a) { %>
    <tr>
      <td><%= a.name %></td>
      <td><%= a.genre %></td>
      <td><%= a.yearFormed %></td>
    </tr>
    <% }) %>
  </tbody>
</table>

<%- include('../partials/footer') %>